package usingInterface;

public class Person {
	
	private String name;
	private int age;
	private boolean gender;
	
	public String getName() {
		return name;
	}
	public int getAge() {
		return age;
	}
	
	public void setAge() {
		age++;
	}
	public boolean isGender() {
		return gender;
	}
	
	public void happyBirthday() {
		setAge();
	}
}
